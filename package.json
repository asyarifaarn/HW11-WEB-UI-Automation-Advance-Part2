{
    "name": "selenium-mocha-digital-skola",
    "version": "1.0.0",
    "type": "module",
    "description": "Selenium WebDriver test using Mocha with Mochawesome reports",
    "scripts": {
        "test": "mocha test/sesi-11/test-hw11.js --recursive --timeout 60000",
        "test-visual": "mocha test/sesi-11/test-hw11.js --recursive --timeout 60000",
        "test-parallel": "mocha test --parallel --recursive --timeout 60000",
        "test-pertama": "mocha test --grep 'Pertama' --recursive --timeout 60000",
        "running-mochawesome": "mocha test --recursive --timeout 60000 --reporter mochawesome",
        "running-simple-html": "mocha test --recursive --timeout 60000 --reporter mocha-simple-html-reporter --reporter-options output=report.html"
    },
    "dependencies": {
        "madge": "^8.0.0",
        "pixelmatch": "^7.1.0",
        "pngjs": "^7.0.0",
        "selenium-webdriver": "^4.17.0"
    },
    "devDependencies": {
        "mocha": "latest",
        "mocha-simple-html-reporter": "^2.0.0",
        "mochawesome": "^7.1.3"
    }
}
